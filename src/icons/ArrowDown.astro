---
interface Props {
	width: string
	height: string
	className?: string
}

const { width, height, className } = Astro.props
---

<svg
  width={width}
  height={height}
  viewBox="0 0 24 24"
  fill="none"
  stroke="currentColor"
  stroke-width="2"
  stroke-linecap="round"
  stroke-linejoin="round"
  class={className}
>
  <path stroke="none" d="M0 0h24v24H0z" fill="none"></path>
  <path d="M12 5l0 14"></path>
  <path d="M18 13l-6 6"></path>
  <path d="M6 13l6 6"></path>
</svg>
